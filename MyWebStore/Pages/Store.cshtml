@page
@model StoreModel

<h1>Store</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col" class="fw-bold">Product Name</th>
            <th scope="col" class="fw-bold">Cost</th>
        </tr>
    </thead>
    <tbody class="table-group-divider">
        @foreach (var product in Model.Products)
        {
            <tr>
                <td>@product.ProductName</td>
                <td>@product.Cost</td>
            </tr>
        }
    </tbody>
</table>